{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "noImplicitAny": false,
    "resolveJsonModule": true
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
